<?xml version="1.0" encoding="UTF-8"?>
<codesense>

<!--Controllers -->

<provider>
  <selector>container.class.test_functional</selector>
    <completions>com.rails.asserts.actiondispatch.select</completions>
    <completions>com.rails.asserts.actiondispatch.select_rjs</completions>
    <completions>com.rails.asserts.actiondispatch.css_select</completions>
    <completions>com.rails.asserts.actiondispatch.select_encoded</completions>
    <completions>com.rails.asserts.actiondispatch.select_encoded</completions>
    <completions>com.rails.asserts.actiondispatch.select_email</completions>
    <completions>com.rails.asserts.actiondispatch.assert_tag</completions>
    <completions>com.rails.asserts.actiondispatch.assert_no_tag</completions>
    <completions>com.rails.asserts.actiondispatch.assert_generates</completions>
    <completions>com.rails.asserts.actiondispatch.assert_recognizes</completions>
    <completions>com.rails.asserts.actiondispatch.assert_routing</completions>
    <completions>com.rails.asserts.actiondispatch.assert_response</completions>
    <completions>com.rails.asserts.actiondispatch.assert_redirected_to</completions>
	<complete-match capture="1">^\s*(?!\.)([a-z]\w*)</complete-match>
</provider>

<!--All-->

<provider>
  <selector>container.class.test_functional, container.class.test_unit</selector>
  <completions>com.rails.asserts.actiondispatch.assert_valid</completions>
  <completions>com.rails.asserts.activesupport.assert_blank</completions>
  <completions>com.rails.asserts.activesupport.assert_difference</completions>
  <completions>com.rails.asserts.activesupport.assert_no_difference</completions>
  <completions>com.rails.asserts.activesupport.assert_present</completions>
	<complete-match capture="1">^\s*(?!\.)([a-z]\w*)</complete-match>
</provider>

</codesense>
